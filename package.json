{
  "name": "@localazy/conventional-changelog-preset",
  "version": "1.0.0",
  "description": "Conventional Changelog with Emojis support",
  "author": "Localazy <team@localazy.com>",
  "license": "MIT",
  "main": "src/plugin.js",
  "files": [
    "src"
  ],
  "scripts": {
    "changelog:preview": "conventional-changelog -p @localazy/preset -u",
    "changelog:regenerate": "conventional-changelog -p @localazy/preset -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "cosmiconfig": "^8.3.6",
    "git-semver-tags": "^7.0.1"
  },
  "devDependencies": {
    "@localazy/conventional-changelog-preset": "github:localazy/conventional-changelog-preset",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-writer": "github:localazy/conventional-changelog-writer",
    "conventional-recommended-bump": "^9.0.0"
  }
}
